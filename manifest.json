{
    "manifest_version": 2,
    "name": "Budget Manager",
    "version": "1.0",
    "description": "This extension will track your overall spending's.",
    "icons": {
        "128": "logo_128.png",
        "48": "logo_128.png",
        "16": "logo_128.png"
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "logo_128.png"
    },
    "permissions": ["storage"]
}
